stify-content: space-between;\r\n  /* border: 1px solid red ; */\r\n}\r\n@media (min-width: 500px) {\r\n  .siteList {\r\n    justify-content: flex-start;\r\n    margin-top: 60px;\r\n    margin-left: 0;\r\n    margin-right: -25px; /*将元素向左拖拽25像素，元素本身的位置不变*/\r\n  }\r\n}\r\n.siteList > li {\r\n  /* border: 1px solid blue; */\r\n  margin-bottom: 10px;\r\n}\r\n@media (min-width: 500px) {\r\n  .siteList > li {\r\n    margin-right: 25px;\r\n  }\r\n}\r\n.siteList .site {\r\n  /* border: 1px solid red ; */\r\n  width: 160px;\r\n  display: flex;\r\n  justify-content: center;\r\n  align-items: center;\r\n  flex-direction: column;\r\n  background-color: #fff;\r\n  margin-bottom: 10px;\r\n  border: 1px solid #ddd;\r\n  border-radius: 4px;\r\n  padding: 10px 0;\r\n  position: relative;\r\n  cursor: pointer;\r\n}\r\n\r\n.siteList .site > .logo {\r\n  width: 64px;\r\n  height: 64px;\r\n  font-size: 64px;\r\n  display: flex;\r\n  justify-content: center;\r\n  align-items: center;\r\n}\r\n.siteList .site > .link {\r\n  font-size: 14px;\r\n  margin-top: 4px;\r\n}\r\n.siteList .site > .close {\r\n  position: absolute;\r\n  top: 0px;\r\n  right: 0px;\r\n  display: none;\r\n}\r\n.siteList .site:hover > .close {\r\n  display: block;\r\n  cursor: default;\r\n}\r\n.siteList .addButton {\r\n  width: 160px;\r\n  display: flex;\r\n  justify-content: center;\r\n  align-items: center;\r\n  flex-direction: column;\r\n  border: 1px solid #ddd;\r\n  padding: 10px 0;\r\n}\r\n.siteList .addButton .iconWrapper {\r\n  display: flex;\r\n  justify-content: center;\r\n  align-items: center;\r\n}\r\n.siteList .addButton .icon {\r\n  width: 64px;\r\n  height: 64px;\r\n}\r\n.siteList .addButton .text {\r\n  font-size: 14px;\r\n  margin-top: 4px;\r\n}\r\n"},"lineCount":null}},"error":null,"hash":"0d3f81c06e98462bb3745b0bccdaf19a","cacheData":{"env":{}}}